{
  "name": "investment-portfolio-manager",
  "version": "1.3.5",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.13.2",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.5",
    "react": "^19.2.3",
    "react-datepicker": "^8.10.0",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-select": "^5.10.2",
    "recharts": "^3.6.0",
    "style-loader": "^4.0.0",
    "web-vitals": "^5.1.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "analyze": "webpack-bundle-analyzer dist/main.js.LICENSE.txt --mode static",
    "test": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=html text",
    "test:coverage:summary": "jest --coverage --coverageReporters=json-summary text",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "lint:jsdoc": "eslint src/**/*.js --rule 'jsdoc/require-jsdoc: error'",
    "docs:coverage": "bash scripts/jsdoc-coverage.sh",
    "format": "prettier --write src/**/*.{js,css}"
  },
  "devServer": {
    "setupMiddlewares": true
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/App.js",
      "!src/config.js",
      "!src/utils/api.js",
      "!src/pages/**/*.{js,jsx}",
      "!src/components/CollapsibleInfo.js",
      "!src/components/FilterPopup.js",
      "!src/components/HealthCheckError.js",
      "!src/components/Navigation.js",
      "!src/components/NumericInput.js",
      "!src/components/StatusTab.js",
      "!src/components/ValueChart.js",
      "!src/components/VersionBanner.js",
      "!src/components/portfolio/DividendsTable.js",
      "!src/components/portfolio/PortfolioActions.js",
      "!src/components/portfolio/TransactionsTable.js",
      "!src/context/ThemeContext.js",
      "!src/**/*index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/pages/",
      "/src/App.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 84,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ]
  }
}
